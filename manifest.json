{
  "manifest_version": 2,
  "name": "kore",
  "version": "1.0",
  "description": "reading helper",
  "permissions": [
    "<all_urls>",
    "activeTab",
    "storage",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "all_frames": true,
      "run_at": "document_start",
      "js": [
        "dist/index.js"
      ]
    }
  ]
}
